#include <stdio.h>
#define MAX 5
int stack[MAX];
int top = -1;
void push(int value) {
 if(top == MAX-1) 
 printf("Stack Overflow\n");
 else {
 stack[++top] = value;
 printf("%d pushed\n", value);
 }
}
void display() {
 if(top == -1) 
 printf("Stack is empty\n");
 else {
 printf("Stack (top -> bottom): ");
 for(int i=top; i>=0; i--)
 printf("%d ", stack[i]);
 printf("\n");
 }
}
int main(){
    push(10);
    push(20);
    push(30);
    display();
    return 0;
}
output:
10 pushed
20 pushed
30 pushed
Stack (top -> bottom): 30 20 10 
